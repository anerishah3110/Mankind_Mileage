<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-configuration PUBLIC
"-//Hibernate/Hibernate Configuration DTD//EN"
"http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">

<hibernate-configuration>
	<session-factory>
		<property name="hibernate.connection.driver_class">

			com.mysql.jdbc.Driver</property>
		<property name="hibernate.connection.url">

			jdbc:mysql://localhost/mankind</property>
		<property name="hibernate.connection.username">root</property>
		<property name="hibernate.connection.password"></property>
		<property name="hibernate.connection.pool_size">10</property>
		<property name="show_sql">true</property>
		<property name="dialect">org.hibernate.dialect.MySQLDialect</property>
		<property name="hibernate.hbm2ddl.auto">update</property>
		<!-- Mapping files -->
	<!--	<mapping resource="vo/country.hbm.xml"/>
		<mapping resource="vo/state.hbm.xml"/>
	 	<mapping resource="vo/city.hbm.xml"/> -->
		<mapping resource="vo/category.hbm.xml"/>
		<mapping resource="vo/subcat.hbm.xml"/>
		<mapping resource="vo/package.hbm.xml"/>
		<mapping resource="vo/event.hbm.xml"/>
		<mapping resource="vo/tutorial.hbm.xml"/>
		<mapping resource="vo/country.hbm.xml"/>
		<mapping resource="vo/state.hbm.xml"/>
		<mapping resource="vo/city.hbm.xml"/>
		<mapping resource="vo/video.hbm.xml"/>
		<mapping resource="vo/user_master.hbm.xml"/>
		<mapping resource="vo/NormalWithoutSibling.hbm.xml"/>
		<mapping resource="vo/CordbloodStorage.hbm.xml"/>
		<mapping resource="vo/report.hbm.xml"/>
		<mapping resource="vo/donation.hbm.xml"/>
		<mapping resource="vo/labreg.hbm.xml"/>
		<mapping resource="vo/siblingmap.hbm.xml"/>
		<mapping resource="vo/image.hbm.xml"/>
		<mapping resource="vo/reimbursement.hbm.xml"/>
		<mapping resource="vo/complaint.hbm.xml"/>
		<mapping resource="vo/feedback.hbm.xml"/>
		<mapping resource="vo/packagePurchase.hbm.xml"/>
		<mapping resource="vo/count.hbm.xml"/>
		<!-- <mapping resource="VO/subcategoryVO.hbm.xml"/>
		<mapping resource="VO/packageVO.hbm.xml"/>
		<mapping resource="VO/brandVO.hbm.xml"/>-->
	</session-factory>
</hibernate-configuration>